{
    "name": "Chromesoul",
    "version": "0.4.3",
    "minimum_chrome_version": "24",
    "manifest_version": 2,
    "offline_enabled": false,
    "description": "Netsoul client for Google Chrome.",
    "icons": { 
	"128": "icon_128.png" 
    },
    "app": {
	"background": {
	    "scripts": ["chromesoul.js"]
	}
    },
    "permissions": [
	"http://www.epitech.eu/intra/photos/",
	"background",
	"storage",
	"notifications",
	{"socket": ["tcp-connect:ns-server.epita.fr:4242"]}
    ]
}
